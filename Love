local currentQuotes = quotesEN;
local countryCode = "Unknown";
local success, result = pcall(function()
	return LocalizationService:GetCountryRegionForPlayerAsync(Player);
end);
if (success and (result == "VN")) then
	currentQuotes = quotesVN;
	print("PhÃ¡t hiá»‡n ngÆ°á»i chÆ¡i tá»« Viá»‡t Nam. Sá»­ dá»¥ng tiáº¿ng Viá»‡t.");
else
	currentQuotes = quotesEN;
	print("PhÃ¡t hiá»‡n ngÆ°á»i chÆ¡i quá»‘c táº¿ (" .. tostring(result) .. "). Sá»­ dá»¥ng tiáº¿ng Anh.");
end
math.randomseed(os.time());
local randomMessage = currentQuotes[math.random(1, #currentQuotes)];
if PlayerGui:FindFirstChild("PinkLoveGui") then
	PlayerGui.PinkLoveGui:Destroy();
end
local gui = Instance.new("ScreenGui");
gui.Name = "PinkLoveGui";
gui.IgnoreGuiInset = true;
gui.ResetOnSpawn = false;
gui.Parent = PlayerGui;
local bg = Instance.new("Frame");
bg.Name = "Background";
bg.Size = UDim2.fromScale(1, 1);
bg.BackgroundColor3 = Color3.fromRGB(255, 255, 255);
bg.Parent = gui;
local gradient = Instance.new("UIGradient");
gradient.Color = ColorSequence.new({ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 182, 193)),ColorSequenceKeypoint.new(1, Color3.fromRGB(255, 105, 180))});
gradient.Rotation = 45;
gradient.Parent = bg;
local textLabel = Instance.new("TextLabel");
textLabel.AnchorPoint = Vector2.new(0.5, 0.5);
textLabel.Position = UDim2.fromScale(0.5, 0.5);
textLabel.Size = UDim2.fromScale(0.85, 0.6);
textLabel.BackgroundTransparency = 1;
textLabel.TextColor3 = Color3.fromRGB(255, 255, 255);
textLabel.Font = Enum.Font.GothamBold;
textLabel.TextWrapped = true;
textLabel.TextScaled = true;
textLabel.TextTransparency = 1;
textLabel.Text = randomMessage;
textLabel.Parent = bg;
local stroke = Instance.new("UIStroke");
stroke.Thickness = 3;
stroke.Color = Color3.fromRGB(255, 80, 120);
stroke.Transparency = 1;
stroke.Parent = textLabel;
local closeBtn = Instance.new("TextButton");
closeBtn.AnchorPoint = Vector2.new(1, 0);
closeBtn.Position = UDim2.fromScale(0.95, 0.05);
closeBtn.Size = UDim2.fromScale(0.08, 0.08);
closeBtn.SizeConstraint = Enum.SizeConstraint.RelativeXX;
closeBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255);
closeBtn.TextColor3 = Color3.fromRGB(255, 100, 100);
closeBtn.Text = "X";
closeBtn.Font = Enum.Font.GothamBlack;
closeBtn.TextScaled = true;
closeBtn.Parent = bg;
local corner = Instance.new("UICorner");
corner.CornerRadius = UDim.new(1, 0);
corner.Parent = closeBtn;
closeBtn.MouseButton1Click:Connect(function()
	local fadeInfo = TweenInfo.new(0.5);
	TweenService:Create(bg, fadeInfo, {BackgroundTransparency=1}):Play();
	TweenService:Create(textLabel, fadeInfo, {TextTransparency=1}):Play();
	TweenService:Create(stroke, fadeInfo, {Transparency=1}):Play();
	TweenService:Create(closeBtn, fadeInfo, {TextTransparency=1,BackgroundTransparency=1}):Play();
	local musicCheck = SoundService:FindFirstChild("LoveMusic");
	if musicCheck then
		TweenService:Create(musicCheck, TweenInfo.new(1), {Volume=0}):Play();
		task.delay(1, function()
			musicCheck:Stop();
		end);
	end
	task.wait(0.5);
	gui:Destroy();
end);
local music = SoundService:FindFirstChild("LoveMusic") or Instance.new("Sound");
music.Name = "LoveMusic";
music.SoundId = "rbxassetid://7453158420";
music.Volume = 0;
music.Looped = true;
music.Parent = SoundService;
music:Play();
TweenService:Create(music, TweenInfo.new(2), {Volume=1}):Play();
TweenService:Create(textLabel, TweenInfo.new(2), {TextTransparency=0}):Play();
TweenService:Create(stroke, TweenInfo.new(2), {Transparency=0}):Play();
local icons = {"â¤","ğŸ’–","ğŸ’•","ğŸŒ¸","ğŸ’Œ","âœ¨","ğŸŒ¹","ğŸ¦‹","ğŸ’˜"};
task.spawn(function()
	while gui.Parent do
		local particle = Instance.new("TextLabel");
		particle.Text = icons[math.random(1, #icons)];
		particle.Font = Enum.Font.GothamBold;
		particle.TextColor3 = Color3.fromHSV(math.random(0.9, 1), math.random(0.2, 0.6), 1);
		particle.TextScaled = true;
		particle.BackgroundTransparency = 1;
		local sizeNum = math.random(4, 9) / 100;
		particle.Size = UDim2.fromScale(sizeNum, sizeNum);
		particle.Position = UDim2.fromScale(math.random(), 1.1);
		particle.Rotation = math.random(-30, 30);
		particle.Parent = bg;
		local speed = math.random(4, 8);
		local tween = TweenService:Create(particle, TweenInfo.new(speed, Enum.EasingStyle.Linear), {Position=UDim2.fromScale(particle.Position.X.Scale, -0.15),Rotation=math.random(-180, 180),TextTransparency=1});
		tween:Play();
		Debris:AddItem(particle, speed);
		task.wait(math.random(1, 4) / 10);
	end
end);
